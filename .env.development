# Development Environment Variables
NODE_ENV=development

# Parser Settings (same as production but with debug logging)
MIN_REQUEST_INTERVAL=100
REQUEST_TIMEOUT=10000
MAX_RETRY_ATTEMPTS=1

# Security (more permissive for development)
USER_AGENT=Mozilla/5.0 (compatible; TrackerParse/1.0)

# Rate Limiting (more generous for development)
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100

# Features
ENABLE_DEBUG_LOGS=true
ENABLE_CONSOLE_LOGS=true

# Development Security (disable CSP for extensions)
DISABLE_CSP=true
